ers/${id}`)\r\n          .then(res => {\r\n               \r\n               console.log('user =>',res.data.data)\r\n               commit('SET_USER',res.data.data)\r\n          })\r\n          .catch(err => {\r\n               console.error(err); \r\n          })\r\n     },\r\n     paginateUser({commit},id){\r\n          axios.get(`https://reqres.in/api/users?page=${id}&per_page=7`)\r\n          .then(res => {\r\n               commit('fetchAllUser',res.data.data)\r\n          })\r\n          .catch(err => {\r\n               console.error(err); \r\n          })\r\n     },\r\n     deleteUser({commit},id){\r\n          axios.delete(`https://reqres.in/api/users/${id}`)\r\n          .then(res => {\r\n               console.log(res)\r\n               commit('deleteUser',id)\r\n          })\r\n          .catch(err => {\r\n               console.error(err); \r\n          })\r\n     },\r\n     addUser({commit},payload){\r\n          axios.post(`https://reqres.in/api/users`,payload)\r\n          .then(res => {\r\n               console.log('add',res)\r\n               commit('ADD_USER',payload)\r\n          })\r\n          .catch(err => {\r\n               console.error(err); \r\n          })\r\n     },\r\n     updateUser({commit},data){\r\n          axios.patch(`https://reqres.in/api/users/${data.id}`,data)\r\n          .then(res => {\r\n               console.log('update',res)\r\n               commit('UPDATE_USER',data)\r\n          })\r\n          .catch(err => {\r\n               console.error(err); \r\n          })\r\n     },\r\n}"]}]}