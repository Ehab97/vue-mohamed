c/components/log","sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\">\r\n         <h3 >log in</h3>\r\n      <b-form-group\r\n        id=\"input-group-1\"\r\n        label=\"Email address:\"\r\n        label-for=\"input-1\"\r\n        description=\"We'll never share your email with anyone else.\"\r\n      >\r\n        <b-form-input\r\n          id=\"input-1\"\r\n          v-model=\"email\"\r\n          type=\"email\"\r\n          placeholder=\"Enter email\"\r\n          required\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n       <label for=\"text-password\">Password</label>\r\n    <b-form-input type=\"password\" id=\"text-password\" v-model=\"password\" aria-describedby=\"password-help-block\"></b-form-input>\r\n    <b-form-text id=\"password-help-block\">\r\n      Your password must be 8-20 characters long, contain letters and numbers, and must not\r\n      contain spaces, special characters, or emoji.\r\n    </b-form-text>\r\n          <div class=\"w-100 mt-3 mb-1\">\r\n               <b-button type=\"submit\" class=\"mr-1\" variant=\"primary\">Submit</b-button>\r\n               <b-button type=\"reset\"  class=\"ml-1\" variant=\"danger\">Reset</b-button>\r\n          </div>\r\n      <router-link to='/signup'>you don't have an account?</router-link>\r\n    </b-form>\r\n\r\n <transition name=\"error\"  mode=\"out-in\">\r\n    <b-alert show variant=\"danger\" v-if=\"error\">{{this.error}}</b-alert>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from 'vuex'\r\n  export default {\r\n    data() {\r\n      return {\r\n          email: '',\r\n          password: ''  ,\r\n          error:null\r\n      }\r\n    },\r\n    methods: {\r\n      onSubmit(event) {\r\n           event.preventDefault()\r\n           if(this.email!=''&&this.password!=''){\r\n               let payload={email:this.email,password:this.password}\r\n               this.logIn(payload);\r\n                  setTimeout(() => {\r\n                    if(this.isLogged) {\r\n                    this.$router.push({name: 'Home'});\r\n                    } else {\r\n                    this.error = 'Please Enter Valid username OR Passowrd';\r\n                    }\r\n               }, 800); \r\n           }else{\r\n                this.error='please enter email and password';\r\n           }\r\n\r\n\r\n      },\r\n      onReset(event) {\r\n        event.preventDefault()\r\n        // Reset our form values\r\n        this.email = ''\r\n        this.password= ''\r\n      },\r\n      ...mapActions('auth', ['logIn'])\r\n    }\r\n     ,computed: {\r\n           ...mapGetters('auth',['isLogged','errors'])\r\n     }\r\n  }\r\n</script>\r\n<style scoped>\r\n.content{\r\n     flex-direction: column;\r\n     display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100vh;\r\n}\r\nform{\r\n     margin-bottom: 25px;;\r\n     width: 50%;\r\n    border-radius: 10px;\r\n    border: 1px solid #ededed;\r\n    padding: 1rem;\r\n}\r\n/* ------------ */\r\n.error-enter-active {\r\n  animation: error .5s ease-out;\r\n}\r\n\r\n/* #2 for leaving elemnt */\r\n\r\n.error-leave-active {\r\n  animation: error .5s ease-in reverse;\r\n} \r\n\r\n@keyframes error {\r\n  0% {\r\n      /* transform: translateY(0) scale(1); */\r\n       opacity: 0;\r\n    /*transform: translateY(30px); */\r\n\r\n    }\r\n\r\n    70% {\r\n      opacity: .5;\r\n      /* transform: translateY(120px) scale(1.1); */\r\n    }\r\n\r\n    100% {\r\n      /* transform: translateY(-150px) scale(1); */\r\n      opacity: 1;\r\n    /*transform: translateY(0); */\r\n    }\r\n}\r\n</style>"]}]}